# Production Environment Variables for RENDEREEL STUDIO Frontend

# Backend API Configuration
REACT_APP_BACKEND_URL=https://rendereelstudio.com
VITE_REACT_APP_BACKEND_URL=https://rendereelstudio.com

# Application Configuration
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production
REACT_APP_APP_NAME=RENDEREEL STUDIO

# Feature Flags
REACT_APP_ENABLE_NSFW=true
REACT_APP_ENABLE_RUNWAY_GEN3=true
REACT_APP_ENABLE_AI_CHAT=true
REACT_APP_ENABLE_SOCIAL_FEATURES=true
REACT_APP_ENABLE_MARKETPLACE=true

# Analytics and Tracking (add when ready)
# REACT_APP_GOOGLE_ANALYTICS_ID=
# REACT_APP_MIXPANEL_TOKEN=

# External Services (add your actual keys when ready)
# REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_live_
# REACT_APP_GOOGLE_CLIENT_ID=

# Performance and Optimization
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_ENABLE_OFFLINE_MODE=false

# UI Configuration
REACT_APP_DEFAULT_THEME=dark
REACT_APP_BRAND_COLOR=#8B5CF6
REACT_APP_SECONDARY_COLOR=#06B6D4

# Content and Limits
REACT_APP_MAX_FILE_SIZE=50000000
REACT_APP_SUPPORTED_IMAGE_FORMATS=jpg,jpeg,png,webp
REACT_APP_SUPPORTED_VIDEO_FORMATS=mp4,mov,avi

# Social Media Integration (update when accounts are created)
REACT_APP_TWITTER_HANDLE=@RendereeelStudio
REACT_APP_DISCORD_INVITE=https://discord.gg/rendereel
REACT_APP_GITHUB_REPO=https://github.com/yourusername/rendereel-studio

# CDN and Assets
REACT_APP_CDN_URL=https://cdn.rendereelstudio.com
REACT_APP_ASSETS_URL=https://assets.rendereelstudio.com

# Legal and Compliance
REACT_APP_PRIVACY_POLICY_URL=https://rendereelstudio.com/privacy
REACT_APP_TERMS_OF_SERVICE_URL=https://rendereelstudio.com/terms
REACT_APP_SUPPORT_EMAIL=support@rendereelstudio.com

# WebSocket Configuration
REACT_APP_WS_URL=wss://rendereelstudio.com/ws

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=8192
